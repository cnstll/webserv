server {
  listen 0.0.0.0:18000;
  server_name webserv;
  root ./forest/iAmRoot;

  location /hash.py {
    root ./forest/cgi;
  }

  location /upload_resource.py {
    root ./forest/cgi;
    upload_dir ./forest/iAmUploadDir;
  }

  location /favicon.ico {
    root ./forest/favicon;
  }

}

server {
  listen 0.0.0.0:18001;
  server_name webserv;
  root ./core/server_root;

  location /hash.py {
    root ./forest/cgi;
  }
}